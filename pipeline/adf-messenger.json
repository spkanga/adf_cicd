{
	"name": "adf-messenger",
	"properties": {
		"activities": [
			{
				"name": "Messenger",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": "https://prod-106.westeurope.logic.azure.com:443/workflows/19210ef00dd9456a955ed70622878540/triggers/manual/paths/invoke?api-version=2016-10-01&sp=%2Ftriggers%2Fmanual%2Frun&sv=1.0&sig=jh9-sENDFE4lmnW_mTXrL5LlaTucSnE11E8d4LP9ysE",
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"body": {
						"DataFactoryName": "@{pipeline().DataFactory}",
						"PipelineName": "@{pipeline().Pipeline}",
						"Subject": "@{pipeline().parameters.Subject}",
						"ErrorMessage": "@{pipeline().parameters.ErrorMessage}",
						"EmailTo": "@pipeline().parameters.EmailTo"
					}
				}
			}
		],
		"parameters": {
			"ErrorMessage": {
				"type": "string",
				"defaultValue": "n"
			},
			"EmailTo": {
				"type": "string",
				"defaultValue": "info@kpsdata.co.uk"
			},
			"Subject": {
				"type": "string",
				"defaultValue": "x"
			}
		},
		"annotations": []
	}
}